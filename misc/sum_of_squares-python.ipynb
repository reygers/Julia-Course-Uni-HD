    "import time\n",
    "\n",
    "def sum_of_squares(n):\n",
    "    total = 0\n",
    "    for i in range(1, n+1):\n",
    "        total += i**2\n",
    "    return total\n",
    "\n",
    "start_time = time.time()\n",
    "print(sum_of_squares(10**5))\n",
    "print(\"Elapsed time for computation:\", time.time() - start_time)"
